{
  "name": "source-map",
  "version": "0.0.0",
  "description": "Created by create-n",
  "keywords": [
    "source-map",
    "course",
    "vivaxy"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --collect-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivaxy/course.git"
  },
  "author": "vivaxy <xyxuye2007@126.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vivaxy/course/issues"
  },
  "homepage": "https://github.com/vivaxy/course#readme",
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/__tests__/**/*.ts"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.0",
    "@types/jest": "^26.0.0",
    "@types/webpack": "^5.0.0",
    "fs-extra": "^9.0.0",
    "jest": "^26.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.0.0",
    "uglifyjs": "^2.4.11",
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "@vivaxy/vlq": "^1.1.0"
  }
}
